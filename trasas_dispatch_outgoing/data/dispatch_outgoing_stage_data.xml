<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="outgoing_stage_draft" model="trasas.dispatch.outgoing.stage">
            <field name="name">Dự thảo</field>
            <field name="sequence">10</field>
            <field name="is_draft" eval="True"/>
        </record>

        <record id="outgoing_stage_waiting_approval" model="trasas.dispatch.outgoing.stage">
            <field name="name">Chờ duyệt</field>
            <field name="sequence">20</field>
        </record>

        <record id="outgoing_stage_approved" model="trasas.dispatch.outgoing.stage">
            <field name="name">Đã duyệt</field>
            <field name="sequence">30</field>
        </record>

        <record id="outgoing_stage_to_promulgate" model="trasas.dispatch.outgoing.stage">
            <field name="name">Chờ ban hành</field>
            <field name="sequence">40</field>
        </record>

        <record id="outgoing_stage_processing" model="trasas.dispatch.outgoing.stage">
            <field name="name">Đang xử lý</field>
            <field name="sequence">50</field>
        </record>

        <record id="outgoing_stage_released" model="trasas.dispatch.outgoing.stage">
            <field name="name">Đã phát hành</field>
            <field name="sequence">60</field>
        </record>

        <record id="outgoing_stage_sent" model="trasas.dispatch.outgoing.stage">
            <field name="name">Đã gửi</field>
            <field name="sequence">70</field>
        </record>

        <record id="outgoing_stage_done" model="trasas.dispatch.outgoing.stage">
            <field name="name">Hoàn thành</field>
            <field name="sequence">80</field>
            <field name="is_done" eval="True"/>
            <field name="fold" eval="True"/>
        </record>

        <record id="outgoing_stage_cancel" model="trasas.dispatch.outgoing.stage">
            <field name="name">Đã hủy</field>
            <field name="sequence">90</field>
            <field name="is_cancel" eval="True"/>
            <field name="fold" eval="True"/>
        </record>
    </data>
</odoo>
